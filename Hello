-- Expected repository details
local AUTHORIZED_OWNER = "Hahaha2995"  -- Your GitHub username
local AUTHORIZED_REPO = "HelloWorld"   -- Your GitHub repository name

-- Function to check if the URL is valid
local function is_valid_url(url)
    -- Check if the URL matches your GitHub repository
    return url:match("https://raw.githubusercontent.com/" .. AUTHORIZED_OWNER .. "/" .. AUTHORIZED_REPO)
end

-- URL to load the code
local url = "https://raw.githubusercontent.com/Hahaha2995/HelloWorld/main/Hello"

-- Check if the URL is valid
if is_valid_url(url) then
    -- If the URL is valid, load and execute the code
    local script = loadstring(game:HttpGet(url, true))
    script()  -- Execute the loaded script

    -- Print a success message
    print("HAHAHAHAH IT WORKS")
else
    -- If the URL is not valid, deny execution
    error("Unauthorized repository or URL detected. Script execution blocked.")
end
