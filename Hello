-- Define the allowed repository URL (match the exact path)
local allowedRepoURL = "https://raw.githubusercontent.com/Hahaha2995/KeySystem/main/Slave"

-- Fetch the script from the URL
local scriptUrl = "https://raw.githubusercontent.com/Hahaha2995/KeySystem/main/Slave"
local scriptContent = game:HttpGet(scriptUrl, true)

-- Check if the URL matches the allowed repository
if scriptUrl == allowedRepoURL then
    -- If the repository matches, proceed with executing the script
    print("Script is from the allowed repository! Executing...")

    -- Now, you can execute the script, for example, loading the Hello script
    local helloUrl = "https://raw.githubusercontent.com/Hahaha2995/HelloWorld/main/Hello"
    local helloScriptContent = game:HttpGet(helloUrl, true)

    -- Execute the Hello World script
    local success, result = pcall(function()
        loadstring(helloScriptContent)()  -- Execute the Hello script
    end)

    if not success then
        print("Error executing Hello script: " .. result)
    else
        print("Hello World script executed successfully!")
    end
else
    -- If the URL doesn't match, print an error message
    print("This script is not from the allowed repository!")
end