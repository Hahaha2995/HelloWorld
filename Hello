-- Define the URL of the Hello script
local helloUrl = "https://raw.githubusercontent.com/Hahaha2995/HelloWorld/main/Hello"

-- Fetch the content of the Hello script from the URL
local helloScriptContent = game:HttpGet(helloUrl, true)

-- Print the content for debugging purposes
print("Fetched Hello Script Content:\n" .. helloScriptContent)

-- Try loading and executing the Hello script using loadstring()
local success, result = pcall(function()
    local func = loadstring(helloScriptContent)  -- Load the script
    if func then
        func()  -- Execute the loaded script
    else
        warn("Failed to load the script!")
    end
end)

-- Check if the script execution was successful or failed
if not success then
    print("Error executing script: " .. result)
else
    print("Script executed successfully!")
end