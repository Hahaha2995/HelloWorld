local loadScript = syn and syn.loadstring or fluxus and fluxus.loadstring or loadstring

if loadScript then
    local url = "https://raw.githubusercontent.com/Hahaha2995/HelloWorld/main/Hello"
    -- Try to load the script from the URL directly
    local scriptSource = game:HttpGet(url, true)  -- Using HttpGet to fetch the script
    
    if scriptSource then
        local func, err = loadScript(scriptSource)  -- Using loadstring to compile the script

        if func then
            func()  -- Execute the loaded script
            print("ɎÈ§§§ 𝓘𝓣 ꅏꂦꋪꀘꑄ")  -- Custom print statement
        else
            warn("Failed to load the script: " .. err)  -- If loadstring fails, show the error message
        end
    else
        warn("Failed to fetch script")
    end
else
    warn("loadstring function not found!")
end